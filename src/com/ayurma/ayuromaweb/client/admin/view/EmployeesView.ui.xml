<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style field='cssGlobal' src='./resource/css/adminCssGlobal.css'/>

	<ui:style>
		.btnPanel{
			padding:5px;
		}
	</ui:style>
	<g:HTMLPanel>
		 <g:Label styleName="{cssGlobal.sectionHeading}">Employees</g:Label>
		<g:HTMLPanel styleName="{style.btnPanel}">
			<g:Button ui:field='btnAddNewEmployee'>Add New Employee</g:Button>
			<g:Button ui:field='btnBrowseEmployees'>Browse Employees</g:Button>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 